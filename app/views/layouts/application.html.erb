<!DOCTYPE html>
<html>
  <head>
    <title>Woldumar</title>
    <%= stylesheet_link_tag :screen, :cache => 'cache/screen', :media => 'screen, projection' %>
    <%= stylesheet_link_tag :print, :cache => 'cache/print', :media => 'print' %>
    
    <!--[if lt IE 9]>
      <%= stylesheet_link_tag :ie, :cache => 'cache/ie', :media => 'screen, projection' %>
    <![endif]-->
    
    <%= javascript_include_tag :defaults, :cache => 'cache/woldumar' %>
    
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div class='two-col'>
      <div id='header'>
        <h1>This is the Header</h1>
      </div>
      <div id='sidebar'>
        <%= nav_bar %>
      </div>
      <div id='content'>
        <%= yield %>
      </div>
      <div id='footer'>
        Footer
      </div>
    </div>
  </body>
</html>
